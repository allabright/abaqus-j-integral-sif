	pages = {107126},
	file = {Wang et al. - 2020 - A review on mixed mode fracture of metals.pdf:/home/allabright/Zotero/storage/AHPN5KXC/Wang et al. - 2020 - A review on mixed mode fracture of metals.pdf:application/pdf},
}

